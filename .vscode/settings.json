{
  "breadcrumbs.enabled": true,
  "coverage-gutters.coverageReportFileName": "coverage/tmp/*.json",
  "coverage-gutters.lcovname": "",
  "coverage-gutters.showGutterCoverage": false,
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showRulerCoverage": true,
  "coverage-gutters.xmlname": "coverage.xml",
  "debug.allowBreakpointsEverywhere": true,
  "debug.inlineValues": "auto",
  // "debug.javascript.usePreview": true,
  "debug.onTaskErrors": "debugAnyway",
  "debug.openDebug": "openOnDebugBreak",
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "languageDefined",
  "editor.fontFamily": "Source Code Pro",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.quickSuggestionsDelay": 750,
  "editor.rulers": [100],
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "enabled": true, // enable semantic highlighting for all themes
    "rules": {
      // different color for all constants
      "property.readonly": "#35166d",

      // make all symbol declarations bold
      "*.declaration": { "bold": true }
    }
  },
  "editor.snippetSuggestions": "bottom",
  "workbench.editor.decorations.badges": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.docker": true,
    "**/tags": true,
    "**/*.pyc": true,
    "**/*.js": { "when": "$(basename).ts" },
    "**/*.js.map": true
  },
  "git.confirmSync": false,
  "gitlens.advanced.blame.delayAfterEdit": 0,
  "gitlens.blame.avatars": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.liveshare.allowGuestAccess": false,
  "gitlens.statusBar.enabled": false,
  "go.enableCodeLens": {
    "references": false,
    "runtest": false
  },
  "go.useLanguageServer": true,
  "go.languageServerExperimentalFeatures": {
    "diagnostics": false
  },
  "go.lintTool": "revive",
  "go.formatTool": "gofmt",
  "go.delveConfig": {
    // "dlvLoadConfig": {
    //   "followPointers": true,
    //   "maxVariableRecurse": 0,
    //   "maxStringLen": 100,
    //   "maxArrayValues": 64,
    //   "maxStructFields": 50,
    //   "showGlobalVariables": false
    // },
    "apiVersion": 2,
    "showGlobalVariables": false
  },
  "go.coverOnSave": false,
  "go.coverOnTestPackage": true,
  "go.coverageDecorator": {
    "type": "gutter",
    "coveredHighlightColor": "rgba(64,128,128,0.5)",
    "uncoveredHighlightColor": "rgba(128,64,64,0.25)",
    "coveredGutterStyle": "blockgreen",
    "uncoveredGutterStyle": "blockred"
  },
  "go.coverOnSingleTest": false,
  "go.testEnvVars": { "LOG_LEVEL": "INFO", "WORK_DIR": "${workspaceFolder}" },
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.fontSize": 12,
  "php.suggest.basic": false,
  "php.validate.enable": false,

  "prettier.goTemplateBracketSpacing": false,
  "prettier.resolveGlobalModules": true,
  "prettier.jsxSingleQuote": true,
  "prettier.quoteProps": "as-needed",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "python.coverageView.highlightMode": "covered",
  "python.linting.enabled": false,
  "python.linting.lintOnSave": false,
  "python.linting.pylintEnabled": false,
  "search.collapseResults": "alwaysExpand",
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.useGlobalIgnoreFiles": true,
  "search.useReplacePreview": false,
  "search.exclude": {
    "**/.docker": true,
    "**/tags": true,
    // "**/*.js": { "when": "$(basename).ts" },
    "**/*.js.map": true,
    "**/node_modules": true,
    "**/tsconfig.tsbuildinfo": true
  },
  "task.slowProviderWarning": ["typescript"],
  "terminal.integrated.fontFamily": "Source Code Pro",
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.profiles.osx": {
    "fish": {
      "path": "/usr/local/bin/fish",
      "args": ["-l"]
    }
  },
  "typescript.autoClosingTags": false,
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "typescript.tsserver.maxTsServerMemory": 30720,
  "typescript.format.enable": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "typescript.tsdk": "~/.config/yarn/global/node_modules/typescript/lib",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.visual": "line",
  "vim.cursorStylePerMode.visualblock": "line",
  "vim.cursorStylePerMode.visualline": "line",
  "vim.debug.loggingLevelForAlert": "error",
  "vim.debug.loggingLevelForConsole": "info",
  "vim.easymotion": false,
  "vim.enableNeovim": false,
  "vim.history": 100,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.searchHighlightColor": "#990",
  "vim.searchHighlightTextColor": "#333",
  "vim.incsearch": true,
  "vim.leader": "<space>",
  // "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.replaceWithRegister": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<esc>"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.clipboardCopyAction"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["editor.action.clipboardPasteAction"]
    },
    {
      "before": ["<leader>", "s"],
      "after": [":", "s", "o", "r", "t", "<cr>"]
    }
  ],
  "vim.mouseSelectionGoesIntoVisualMode": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<esc>", "<cr>"],
      "after": ["<esc>", ":", "n", "o", "h", "l", "<cr>"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<leader>", "s"],
      "after": [":", "s", "o", "r", "t", "<cr>"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.clipboardCopyAction"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["editor.action.clipboardPasteAction"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.files.action.showActiveFileInExplorer"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["<C-r>"],
      "commands": ["redo"]
    },
    {
      "before": ["U"],
      "commands": ["redo"]
    },
    {
      "before": ["<C-i>"],
      "after": ["<C-o>"]
    },
    {
      "before": ["<C-o>"],
      "after": ["<C-i>"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z"]
    },
    {
      "before": ["n"],
      "after": ["n", "z", "z"]
    }
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.revealIfOpen": true,
  "workbench.fontAliasing": "auto",
  "workbench.list.horizontalScrolling": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f",
    "editor.background": "#161515",
    "editor.lineHighlightBackground": "#44475a"
  },
  "workbench.settings.editor": "json",

  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.autoClosingBrackets": "never"
  },
  "[go]": {
    "editor.autoClosingBrackets": "never"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.autoClosingBrackets": "never"
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    // "editor.defaultFormatter": "vscode.html-language-features",
    "editor.autoSurround": "never"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.toolsManagement.autoUpdate": true,
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "ITerm.app",
  "terminal.integrated.allowChords": false,
  "workbench.editorAssociations": {
    "*.png": "imagePreview.previewEditor"
  },
  "files.associations": {
    "*.gohtml": "html"
  },
  "debug.javascript.unmapMissingSources": true,
  "cmake.configureOnOpen": false
}
