{
  "diffEditor.originalEditable": true,
  "workbench.list.horizontalScrolling": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.clipboardCopyAction"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["editor.action.clipboardPasteAction"]
    }
  ],
  "vim.mouseSelectionGoesIntoVisualMode": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.clipboardCopyAction"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["editor.action.clipboardPasteAction"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.files.action.showActiveFileInExplorer"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["<C-r>"],
      "commands": ["redo"]
    },
    {
      "before": ["U"],
      "commands": ["redo"]
    },
    {
      "before": ["<C-i>"],
      "after": ["<C-o>"]
    },
    {
      "before": ["<C-o>"],
      "after": ["<C-i>"]
    },
    {
      "before": ["N"],
      "after": ["N", "zz"]
    },
    {
      "before": ["n"],
      "after": ["n", "zz"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.debug.loggingLevelForConsole": "info",
  "vim.debug.loggingLevelForAlert": "error",
  "vim.visualstar": true,
  "vim.history": 100,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f",
    "editor.background": "#161515",
    "editor.lineHighlightBackground": "#44475a"
  },
  "go.enableCodeLens": {
    "references": false,
    "runtest": false
  },
  "go.useLanguageServer": true,
  "go.languageServerExperimentalFeatures": {
    "format": true,
    "autoComplete": true,
    "rename": true,
    "goToDefinition": true,
    "hover": true,
    "signatureHelp": true,
    "goToTypeDefinition": true,
    "goToImplementation": true,
    "documentSymbols": true,
    "workspaceSymbols": true,
    "findReferences": true,
    "diagnostics": false,
    "documentLink": true
  },
  "go.lintTool": "revive",
  "go.formatTool": "gofmt",
  "go.delveConfig": {
    "dlvLoadConfig": {
      "followPointers": true,
      "maxVariableRecurse": 3,
      "maxStringLen": 150,
      "maxArrayValues": 150,
      "maxStructFields": -1,
    },
    "apiVersion": 2,
    "showGlobalVariables": false,
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "debug.openDebug": "openOnDebugBreak",
  "debug.inlineValues": false,
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "breadcrumbs.enabled": true,
  "prettier.semi": false,
  "prettier.jsxSingleQuote": false,
  "prettier.quoteProps": "consistent",
  // "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.enableNeovim": false,
  "coverage-gutters.xmlname": "coverage.xml",
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showRulerCoverage": true,
  "coverage-gutters.lcovname": "",
  "coverage-gutters.coverageReportFileName": "coverage/tmp/*.json",
  "coverage-gutters.showGutterCoverage": false,
  "python.coverageView.highlightMode": "covered",
  "files.exclude": {
    "**/__pycache__": true,
    "**/.docker": true,
    "**/tags": true,
    "**/*.pyc": true
  },
  "search.exclude": {
    "**/.docker": true,
    "**/tags": true
  },
  "prettier.singleQuote": true,
  "[go]": {
    "editor.autoClosingBrackets": "never"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.autoClosingBrackets": "never"
  },
  "[less]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.smartCase": true,
  "search.collapseResults": "alwaysExpand",
  "search.runInExtensionHost": true,
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "search.useReplacePreview": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.allowBreakpointsEverywhere": true,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "gitlens.advanced.blame.delayAfterEdit": 0,
  "gitlens.blame.avatars": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.liveshare.allowGuestAccess": false,
  "gitlens.statusBar.enabled": false,
  "editor.fontFamily": "Hack Nerd Font Mono",
  "editor.rulers": [ 100 ],
  "terminal.integrated.fontFamily": "Hack Nerd Font Mono",
  "terminal.integrated.shellArgs.osx": [],
  "window.zoomLevel": 0,
  "markdown.preview.fontSize": 12,
  "vim.easymotionMarkerFontSize": "12",
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.scrollback": 10000,
  "vim.replaceWithRegister": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "task.slowProviderWarning": [
    "typescript"
  ],
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
